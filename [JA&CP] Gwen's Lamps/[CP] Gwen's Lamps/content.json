{
    "Format": "1.24.0",
	
	"ConfigSchema": 
	
	{
		"OldIronFoliage":{"AllowValues": "true, false",
					"Default": "true"},
		"OvalFoliage":{"AllowValues": "true, false",
					"Default": "true"},
		"StoneFoliage":{"AllowValues": "true, false",
					"Default": "true"},
		"FramedFoliage":{"AllowValues": "true, false",
					"Default": "true"},
		"LanternFoliage":{"AllowValues": "true, false",
					"Default": "true"},	
		"BendyLeftFoliage":{"AllowValues": "true, false",
					"Default": "true"},
		"BendyRightFoliage":{"AllowValues": "true, false",
					"Default": "true"},
		"OilRightFoliage":{"AllowValues": "true, false",
					"Default": "true"},
		"OilLeftFoliage":{"AllowValues": "true, false",
					"Default": "true"},

		
					
		"RecolorOverwrite":{"AllowValues": "Vanilla, DNEarthyRecolour, Eemie's, FoliageRedone, Starblue, VibrantPastoral, WittilyNamed",
					"AllowBlank": true,
					"Default": "",
					"Description": " Change ONLY when you want to change automaticaly matched recolor palette" },
	},
	
	"DynamicTokens": [
	
	//checking what recolor is used
		{
		"Name": "recolor",
		"Value": "Vanilla",
		},
		{
		"Name": "recolor",
		"Value": "DNEarthyRecolour",
			"When": { "HasMod": "DaisyNiko.EarthyRecolour" },
		},
		{
		"Name": "recolor",
		"Value": "Eemie's",
			"When": { "HasMod": "minervamaga.CP.eemieMapRecolour" },
		},
		{
		"Name": "recolor",
		"Value": "FoliageRedone",
			"When": { "HasMod": "stardewfoliageredone" },
		},
		{
		"Name": "recolor",
		"Value": "Starblue",
			"When": { "HasMod": "Lita.StarblueValley" },
		},
		{
		"Name": "recolor",
		"Value": "VibrantPastoral",
			"When": { "HasMod": "grapeponta.VibrantPastoralRecolor" },
		},
		{
		"Name": "recolor",
		"Value": "WittilyNamed",
			"When": { "HasMod": "Acerbicon.Recolor" },
		},

	//recolor overwrite
		{
		"Name": "recolor",
		"Value": "{{RecolorOverwrite}}",
			"When": { "RecolorOverwrite |contains=Vanilla, DNEarthyRecolour, Eemie's, FoliageRedone, Starblue, VibrantPastoral, WittilyNamed": true },
		},
	],
	
    "Changes": [
	

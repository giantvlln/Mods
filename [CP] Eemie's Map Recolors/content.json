{
	"Format": "1.11",

	"ConfigSchema": {

		"SpringColor": {
			"AllowValues": "Green, Pink",
			"Default": "Pink"
			},
		"FallColor": {
			"AllowValues": "Green, Orange",
			"Default": "Green"
			},
		"DirtColor": {
			"AllowValues": "Classic, Light",

			},
		"UseFallBushes": {
			"AllowValues": "true, false",
			"Default": true
			},
		"UseEemiePine": {
			"AllowValues": "true, false",
			"Default": true
			},
		"GrassVariant": {
			"AllowValues": "Eemie, AliEemie, AliVibrant, AliNormal, AliAlternate, AliAlternateVibrant, disable",
